\begin_layout Itemize

\series bold
\color blue
Logistic regression
\series default
\color inherit

\begin_inset Formula 
\[
\Pr(y_{i}=1\text{ }|\text{ }x_{i})=\frac{\exp(x_{i}^{\prime}\beta)}{1+\exp(x_{i}^{\prime}\beta)}.
\]

\end_inset


\end_layout

\begin_layout Itemize
Likelihood
\begin_inset Formula 
\[
p(y|X,\beta)=\prod\nolimits _{i=1}^{n}\frac{[\exp(x_{i}^{\prime}\beta)]^{y_{i}}}{1+\exp(x_{i}^{\prime}\beta)}.
\]

\end_inset


\end_layout

\begin_layout Itemize
Prior 
\begin_inset Formula $\beta\sim N(0,\tau^{2}I)$
\end_inset

.
 Posterior is non-standard (see demo in R later).
\end_layout

\begin_layout Itemize
Alternative: 
\series bold
\color blue
Probit regression
\series default
\color inherit

